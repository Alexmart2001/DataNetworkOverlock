            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </ImageView>
        <ImageView fitHeight="68.0" fitWidth="402.0" layoutX="14.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true">
