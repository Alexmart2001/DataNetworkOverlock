                <Cursor fx:constant="HAND" />
            </cursor>
        </ImageView>
        <ImageView fx:id="btnClose" fitHeight="30.0" fitWidth="40.0" layoutX="18.0" layoutY="15.0" onMouseClicked="#cerrar" pickOnBounds="true" preserveRatio="true">
