<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="550" prefWidth="900" style="-fx-background-color: #1f2025;"
            xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="co.edu.usbbog.datanetworkoverlock.view.DashboardController">
    <Pane layoutX="-2.0" prefHeight="550.0" prefWidth="235.0" style="-fx-background-color: #141418;">
        <ImageView fx:id="btnMinimize" fitHeight="30.0" fitWidth="40.0" layoutX="40.0" layoutY="15.0"
                   onMouseClicked="#minimize" pickOnBounds="true" preserveRatio="true">
            <effect>
                <Bloom/>
            </effect>
            <Image url="@../../../../../Iconos/icons8-minimize-window-96.png"/>
        </ImageView>
        <ImageView fx:id="btnClose" fitHeight="30.0" fitWidth="40.0" layoutX="10.0" layoutY="15.0"
                   onMouseClicked="#cerrar" pickOnBounds="true" preserveRatio="true">
            <effect>
                <Bloom/>
            </effect>
            <Image url="@../../../../../Iconos/icons8-close-window-96.png"/>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </ImageView>
        <ImageView fitHeight="44.0" fitWidth="50.0" layoutX="10.0" layoutY="500.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@../../../../../Iconos/icons8-github-96.png"/>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </ImageView>
        <VBox layoutX="5.0" layoutY="107.0" prefHeight="364.0" prefWidth="228.0">
            <Label prefHeight="54.0" prefWidth="235.0"
                   style="-fx-label-padding: 2px 20px; -fx-font-family: 'Montserrat', sans-serif;" styleClass="options"
                   text="Nmap" textFill="WHITE">
                <font>
                    <Font name="SansSerif Regular" size="20.0"/>
                </font>
                <cursor>
                    <Cursor fx:constant="HAND"/>
                </cursor>
            </Label>
            <Label layoutX="10.0" layoutY="10.0" prefHeight="54.0" prefWidth="235.0"
                   style="-fx-label-padding: 2px 20px; -fx-font-family: 'Montserrat', sans-serif;" text="Nessus"
                   textFill="WHITE">
                <font>
                    <Font name="SansSerif Regular" size="20.0"/>
                </font>
                <cursor>
                    <Cursor fx:constant="HAND"/>
                </cursor>
            </Label>
            <Label layoutX="10.0" layoutY="64.0" prefHeight="54.0" prefWidth="235.0"
                   style="-fx-label-padding: 2px 20px; -fx-font-family: 'Montserrat', sans-serif;" text="Kismet"
                   textFill="WHITE">
                <font>
                    <Font name="SansSerif Regular" size="20.0"/>
                </font>
                <cursor>
                    <Cursor fx:constant="HAND"/>
                </cursor>
            </Label>
        </VBox>
    </Pane>
    <Pane layoutX="234.0" prefHeight="550.0" prefWidth="666.0">
        <ImageView fitHeight="68.0" fitWidth="402.0" layoutX="14.0" layoutY="22.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@../../../../../Iconos/overlock.png"/>
        </ImageView>
        <ImageView fitHeight="30.0" fitWidth="40.0" layoutX="576.0" layoutY="22.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@../../../../../Iconos/icons8-add-user-male.png"/>
            <effect>
                <Bloom/>
            </effect>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </ImageView>
        <ImageView fitHeight="30.0" fitWidth="44.0" layoutX="614.0" layoutY="22.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@../../../../../Iconos/icons8-business-report-96.png"/>
            <effect>
                <Bloom/>
            </effect>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </ImageView>
        <TextArea fx:id="codeArea" layoutX="14.0" layoutY="101.0" onKeyPressed="#execute" prefHeight="438.0"
                  prefWidth="637.0" text="admin@dno-$" wrapText="true">
            <font>
                <Font name="Consolas" size="16.0"/>
            </font>
        </TextArea>
    </Pane>
</AnchorPane>
